{
    "id": "camt054-to-mt9x0-fields-mapping",
    "name": "camt.054 to MT900/MT910 Fields Mapping",
    "description": "Maps fields for MT900 and MT910 transformation",
    "priority": 5,
    "condition": { "and": [
        {"in": [{"var": "metadata.SwiftMT.variant"}, ["MT900", "MT910"]]},
        {"==": [{"var": "metadata.ISO20022_MX.message_type"}, "camt.054"]},
        {"==": [{"var": "metadata.direction"}, "incoming"]},
        {"==": [{"var": "metadata.progress.workflow_id"}, "camt054-headers-mapping"]},
        {"==": [{"var": "metadata.progress.status_code"}, 200]}
    ]},
    "tasks": [
        {
            "id": "setup_paths",
            "name": "Setup Paths",
            "description": "Sets up paths to entry and transaction details",
            "function": {
                "name": "map",
                "input": {
                    "mappings": [
                        {
                            "path": "temp_data.entry_path",
                            "logic": {"var": "data.ISO20022_MX.Document.BkToCstmrDbtCdtNtfctn.Ntfctn.0.Ntry.0"}
                        },
                        {
                            "path": "temp_data.txn_path",
                            "condition": {"!!": {"var": "data.ISO20022_MX.Document.BkToCstmrDbtCdtNtfctn.Ntfctn.0.Ntry.0.NtryDtls.0.TxDtls.0"}},
                            "logic": {"var": "data.ISO20022_MX.Document.BkToCstmrDbtCdtNtfctn.Ntfctn.0.Ntry.0.NtryDtls.0.TxDtls.0"}
                        }
                    ]
                }
            }
        },
        {
            "id": "map_reference_fields",
            "name": "Map Reference Fields",
            "description": "Maps field 20 and 21",
            "function": {
                "name": "map",
                "input": {
                    "mappings": [
                        {
                            "path": "data.SwiftMT.fields.#20",
                            "logic": {
                                "reference": { "if": [
                                    {"var": "data.ISO20022_MX.Document.BkToCstmrDbtCdtNtfctn.Ntfctn.0.Id"},
                                    {
                                        "substr": [{"var": "data.ISO20022_MX.Document.BkToCstmrDbtCdtNtfctn.Ntfctn.0.Id"}, 0, 16]
                                    },
                                    { "if": [
                                        {"var": "data.ISO20022_MX.AppHdr.BizMsgIdr"},
                                        {
                                            "substr": [{"var": "data.ISO20022_MX.AppHdr.BizMsgIdr"}, 0, 16]
                                        },
                                        "NOTPROVIDED"
                                    ]}
                                ]}
                            }
                        },
                        {
                            "path": "data.SwiftMT.fields.#21",
                            "logic": {
                                "reference": { "if": [
                                    {"var": "temp_data.txn_path.Refs.AcctSvcrRef"},
                                    {
                                        "substr": [{"var": "temp_data.txn_path.Refs.AcctSvcrRef"}, 0, 16]
                                    },
                                    { "if": [
                                        {"var": "temp_data.txn_path.Refs.InstrId"},
                                        {
                                            "substr": [{"var": "temp_data.txn_path.Refs.InstrId"}, 0, 16]
                                        },
                                        { "if": [
                                            {"var": "temp_data.txn_path.Refs.EndToEndId"},
                                            {
                                                "substr": [{"var": "temp_data.txn_path.Refs.EndToEndId"}, 0, 16]
                                            },
                                            "NOTPROVIDED"
                                        ]}
                                    ]}
                                ]}
                            }
                        }
                    ]
                }
            }
        },
        {
            "id": "map_account_and_amount",
            "name": "Map Account and Amount Fields",
            "description": "Maps field 25 and 32A",
            "function": {
                "name": "map",
                "input": {
                    "mappings": [
                        {
                            "path": "data.SwiftMT.fields.#25",
                            "logic": {
                                "authorisation": { "if": [
                                    {"var": "data.ISO20022_MX.Document.BkToCstmrDbtCdtNtfctn.Ntfctn.0.Acct.Id.IBAN"},
                                    {"var": "data.ISO20022_MX.Document.BkToCstmrDbtCdtNtfctn.Ntfctn.0.Acct.Id.IBAN"},
                                    { "if": [
                                        {"var": "data.ISO20022_MX.Document.BkToCstmrDbtCdtNtfctn.Ntfctn.0.Acct.Id.Othr.Id"},
                                        {"var": "data.ISO20022_MX.Document.BkToCstmrDbtCdtNtfctn.Ntfctn.0.Acct.Id.Othr.Id"},
                                        "NOTPROVIDED"
                                    ]}
                                ]}
                            }
                        },
                        {
                            "path": "data.SwiftMT.fields.32A",
                            "logic": {
                                "value_date": { "if": [
                                    {"var": "temp_data.entry_path.ValDt.Dt"},
                                    {"var": "temp_data.entry_path.ValDt.Dt"},
                                    { "if": [
                                        {"var": "temp_data.txn_path.RltdDts.IntrBkSttlmDt"},
                                        {"var": "temp_data.txn_path.RltdDts.IntrBkSttlmDt"},
                                        {"var": "temp_data.entry_path.BookgDt.Dt"}
                                    ]}
                                ]},
                                "currency": {"var": "temp_data.entry_path.Amt.@Ccy"},
                                "amount": {"var": "temp_data.entry_path.Amt.$value"}
                            }
                        }
                    ]
                }
            }
        },
        {
            "id": "map_mt910_party",
            "name": "Map MT910 Party Fields",
            "description": "Maps field 50 and 52 for MT910",
            "condition": {"==": [{"var": "SwiftMT.variant"}, "MT910"]},
            "function": {
                "name": "map",
                "input": {
                    "mappings": [
                        {
                            "path": "data.SwiftMT.fields.#50",
                            "condition": {"!!": {"var": "temp_data.txn_path.RltdPties.Dbtr.Pty"}},
                            "logic": {
                                "account": {"var": "temp_data.txn_path.RltdPties.Dbtr.Pty.Id.OrgId.Othr.0.Id"},
                                "name_and_address": [
                                    { "if": [
                                        {"var": "temp_data.txn_path.RltdPties.Dbtr.Pty.Nm"},
                                        {"var": "temp_data.txn_path.RltdPties.Dbtr.Pty.Nm"},
                                        "NOTPROVIDED"
                                    ]}
                                ]
                            }
                        },
                        {
                            "path": "data.SwiftMT.fields.52A",
                            "condition": { "and": [
                                {"!": {"!!": {"var": "data.SwiftMT.fields.50"}}},
                                {"!!": {"var": "temp_data.txn_path.RltdPties.Dbtr.Agt"}}
                            ]},
                            "logic": {
                                "bic": { "if": [
                                    {"var": "temp_data.txn_path.RltdPties.Dbtr.Agt.FinInstnId.BICFI"},
                                    {"var": "temp_data.txn_path.RltdPties.Dbtr.Agt.FinInstnId.BICFI"},
                                    "NOTPROVIDED"
                                ]}
                            }
                        }
                    ]
                }
            }
        },
        {
            "id": "map_mt900_party",
            "name": "Map MT900 Party Fields",
            "description": "Maps field 52 for MT900",
            "condition": {"==": [{"var": "SwiftMT.variant"}, "MT900"]},
            "function": {
                "name": "map",
                "input": {
                    "mappings": [
                        {
                            "path": "data.SwiftMT.fields.52D",
                            "condition": { "and": [
                                {"!!": {"var": "temp_data.txn_path.RltdPties.Dbtr.Agt"}},
                                {"!": {"!!": {"var": "temp_data.txn_path.RltdAgts.DbtrAgt"}}}
                            ]},
                            "logic": {
                                "name_and_address": [
                                    { "if": [
                                        {"var": "temp_data.txn_path.RltdPties.Dbtr.Agt.FinInstnId.Nm"},
                                        {"var": "temp_data.txn_path.RltdPties.Dbtr.Agt.FinInstnId.Nm"},
                                        { "if": [
                                            {"var": "temp_data.txn_path.RltdPties.Dbtr.Agt.FinInstnId.BICFI"},
                                            {"var": "temp_data.txn_path.RltdPties.Dbtr.Agt.FinInstnId.BICFI"},
                                            "NOTPROVIDED"
                                        ]}
                                    ]}
                                ]
                            }
                        }
                    ]
                }
            }
        },
        {
            "id": "map_additional_info",
            "name": "Map Additional Information",
            "description": "Maps field 72 if present",
            "function": {
                "name": "map",
                "input": {
                    "mappings": [
                        {
                            "path": "data.SwiftMT.fields.#72",
                            "condition": {"!!": {"var": "temp_data.txn_path.AddtlTxInf"}},
                            "logic": {
                                "information": [
                                    {
                                        "substr": [{"var": "temp_data.txn_path.AddtlTxInf"}, 0, 35]
                                    }
                                ]
                            }
                        }
                    ]
                }
            }
        }
    ]
}
