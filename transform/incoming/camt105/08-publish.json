{
    "id": "camt105-publish",
    "name": "camt.105 to MTn90 Publish",
    "description": "Publishes the final MT190/MT290 message",
    "priority": 9,
    "condition": { "and": [
        {"==": [{"var": "metadata.ISO20022_MX.message_type"}, "camt.105"]},
        {"==": [{"var": "metadata.transformation_direction"}, "mx-to-mt"]},
        {"==": [{"var": "metadata.progress.workflow_id"}, "camt105-postconditions"]},
        {"==": [{"var": "metadata.progress.task_id"}, "finalize_mtn90"]},
        {"==": [{"var": "metadata.progress.status_code"}, 200]}
    ]},
    "tasks": [
        {
            "id": "publish_mtn90",
            "name": "Publish MTn90 Message",
            "description": "Publishes the final MT190/MT290 message",
            "function": {"name": "publish_mt", "input": {"source": "SwiftMT", "target": "result"}}
        }
    ]
}
