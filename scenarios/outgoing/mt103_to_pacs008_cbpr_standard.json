                            {
                                "substr": [{"fake": ["time"]}, 3, 2]
                            }
                        ]
                    },
                    "sign": "+",
                    "offset": "0100"
                }
            ],
            "23B": {"instruction_code": {"var": "bank_op_code"}},
            "32A": {
                "value_date": {"var": "value_date"},
                "currency": {"var": "currency"},
                "amount": {"var": "amount"}
