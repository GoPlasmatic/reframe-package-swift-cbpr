            "50K": {
                "account": {"cat": ["/", {"fake": ["iban"]}]},
                "name_and_address": [
                    {"var": "ordering_customer_name"},
                    {"cat": ["/LEI/", {"var": "ordering_customer_lei"}]},
                    {"var": "ordering_customer_address"},
                    {"var": "ordering_customer_country"}
                ]
            },
            "52A": {"bic": {"var": "sender_bic"}},
            "56A": {"bic": {"var": "intermediary_bic"}},
            "57A": {"bic": {"var": "account_with_bank_bic"}},
            "59": {
                "account": {"cat": ["/", {"fake": ["iban"]}]},
                "name_and_address": [
                    {"var": "beneficiary_name"},
                    {"cat": ["/LEI/", {"var": "beneficiary_lei"}]},
                    {"var": "beneficiary_address"},
                    {"var": "beneficiary_country"}
                ]
            },
            "70": {
                "narrative": [
                    {
                        "substr": [{"cat": ["/RFB/", {"var": "remit_info"}]}, 0, 35]
                    },
                    {
                        "substr": [{"cat": ["/PURP/", {"var": "purpose_code"}]}, 0, 35]
                    },
                    {
                        "substr": [{"cat": ["/ULTD/", {"var": "ordering_customer_lei"}]}, 0, 35]
                    },
                    {
                        "substr": [{"cat": ["/ULTB/", {"var": "beneficiary_lei"}]}, 0, 35]
                    }
                ]
            },
            "71A": {"code": {"var": "charges"}},
            "72": {"information": ["/ACC/CBPR+ COMPLIANT", "/INS/STP PROCESSING", "/REC/FULL TRANSPARENCY"]},
            "77B": {
                "narrative": [
                    {"cat": ["/ORDERRES/", {"var": "ordering_customer_country"}]},
                    {"cat": ["/BENEFRES/", {"var": "beneficiary_country"}]}
                ]
